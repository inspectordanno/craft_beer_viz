{
  "name": "webpack-template",
  "version": "1.0.0",
  "description": "Project template with React, Webpack",
  "main": "index.js",
  "scripts": {
    "test": "mocha './src/**/*.test.js' --compilers js:babel-core/register",
    "test:watch": "npm test -- --watch",
    "start": "webpack-dev-server --config webpack.config.dev.js --inline",
    "build": "webpack -p --config webpack.config.prod.js",
    "dev": "parallelshell 'npm run test:watch' 'npm start'",
    "lint": "eslint src"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "expect": "^24.8.0",
    "file-loader": "^4.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^6.1.4",
    "node-sass": "^4.12.0",
    "parallelshell": "^3.0.2",
    "path": "^0.12.7",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.0.0",
    "webpack": "^4.34.0",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "d3": "^5.9.2",
    "d3-extended": "^2.0.0",
    "d3-svg-legend": "^2.25.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "hoek": "^6.1.3",
    "jquery": "^3.5.0",
    "mime": "^2.4.4",
    "react": "^16.8.6",
    "ssri": "^6.0.1",
    "svg-url-loader": "^2.3.2"
  },
  "babel": {
    "presets": [
      "stage-2",
      "es2015"
    ]
  }
}
